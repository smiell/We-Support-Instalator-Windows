<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
	<ApplicationIcon>Assets\ikona.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\background.png" />
    <None Remove="Assets\ikona.ico" />
    <None Remove="Assets\Poppins_Bold.ttf" />
    <None Remove="Assets\Poppins_Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\background.png" />
    <EmbeddedResource Include="Assets\ikona.ico" />
    <EmbeddedResource Include="Assets\Poppins_Bold.ttf" />
    <EmbeddedResource Include="Assets\Poppins_Regular.ttf" />
  </ItemGroup>

</Project>